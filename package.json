{
  "name": "cooky-cutter",
  "version": "0.0.6",
  "description": "Typed JavaScript object factories for testing",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "preversion": "npm test",
    "version": "npm run build",
    "postversion": "git push origin master --tags",
    "check-types": "npm run build --noEmit",
    "check-formatting": "./node_modules/.bin/prettier --list-different \"src/**/*.ts*\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skovy/cooky-cutter.git"
  },
  "author": "Spencer Miskoviak",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skovy/cooky-cutter/issues"
  },
  "homepage": "https://github.com/skovy/cooky-cutter#readme",
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "prettier": "1.13.7",
    "pretty-quick": "^1.6.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
